import java.util.Scanner;
public class Simulad01 {
    Scanner scanner = System.out.println (new Scanner());
    int valorembrulho = Scanner(System.in);
    System.out.println("O Valor do embrulho é: " + valorembrulho);
    Scanner scanner = System.out.println (new Scanner());
    int valorcompra = Scanner(System.in);
            System.out.println("O Valor da compra do produto é: " + valorcompra);
            diferenca = valorcompra - valorembrulho;
            System.out.println("O Valor a mais do presente é: " + diferenca);
            valortotal = valorcompra + valorembrulho;
            System.out.println("O Valor total em tudo que pagou foi: " valortotal;)
}
